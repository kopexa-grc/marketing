{
  "format": "page",
  "id": "home_page",
  "label": "Home page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "theme": {
        "type": "Select",
        "config": {
          "label": "Theme",
          "placeholder": "",
          "options": ["Inherit from Parent", "Light", "Dark"],
          "default_value": "Inherit from Parent"
        }
      },
      "navbar": {
        "type": "Select",
        "config": {
          "label": "Navbar Theme",
          "placeholder": "",
          "options": ["Inherit from Parent", "Light", "Dark"],
          "default_value": "Inherit from Parent"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "features": {
              "type": "SharedSlice"
            },
            "how_it_works": {
              "type": "SharedSlice"
            },
            "benefits": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            },
            "call_to_action": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
